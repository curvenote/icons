{
  "name": "@curvenote/icons",
  "description": "Curvenote icons for react applications",
  "version": "0.0.3",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src/**/**.ts*",
    "lint:format": "prettier --check \"src/**/*.{ts,tsx,md}\"",
    "dev": "npm-run-all --parallel \"build:* -- --watch\"",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es2020 --outDir dist/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationMap --outDir dist/types",
    "build": "npm-run-all -l clean -p build:cjs build:esm build:types",
    "changeset": "changeset",
    "version": "changeset version && npm install",
    "publish": "npm run build && changeset publish && git push --follow-tags"
  },
  "dependencies": {},
  "peerDependencies": {
    "@types/react": "^16.8 || ^17.0 || ^18.0",
    "@types/react-dom": "^16.8 || ^17.0 || ^18.0",
    "react": "^16.8 || ^17.0 || ^18.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "eslint": "^8.21.0",
    "eslint-config-curvenote": "latest",
    "npm-run-all": "^4.1.5",
    "tsconfig": "latest",
    "typescript": "latest"
  }
}
